{
  "name": "my-ts-lib",
  "version": "0.0.0",
  "description": "My awesome typescript library",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "bin": "./dist/index.mjs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build-fast": "tsup src/index.ts --format cjs,esm",
    "build": "pnpm run build-fast -- --dts-resolve",
    "test": "vitest run",
    "prepublishOnly": "pnpm run build"
  },
  "license": "MIT",
  "devDependencies": {
    "@egoist/prettier-config": "1.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.13.0",
    "prettier": "2.5.1",
    "tsup": "6.4.0",
    "typescript": "4.5.4",
    "vitest": "0.12.6"
  },
  "dependencies": {
    "@clack/prompts": "^0.2.0",
    "emphasize": "^6.0.0",
    "execa": "^7.0.0",
    "fs-extra": "^11.1.0",
    "picocolors": "^1.0.0"
  }
}
